{
  "name": "covid_19_zones",
  "private": true,
  "scripts": {
    "start": "./bin/webpack-dev-server",
    "build": "echo 'run yarn precompile'",
    "precompile": "./bin/rails assets:precompile",
    "fix": "eslint 'src/**/*.{js,ts,tsx}' 'app/javascript/packs/**/*.{js,ts,tsx}' --fix",
    "lint": "eslint --ignore-pattern 'src/**/*.stories.{js,ts,tsx}' 'src/**/*.{js,ts,tsx}' 'app/javascript/packs/**/*.{js,ts,tsx}'",
    "lint:ci": "yarn run eslint --ext .tsx,.ts,.js,.jsx --ignore-pattern '*.stories.tsx' src",
    "guard": "jest src --watchAll",
    "test": "jest --verbose src"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-react": "^7.9.4",
    "@material-ui/core": "^4.9.10",
    "@rails/actioncable": "^6.0.0",
    "@rails/activestorage": "^6.0.0",
    "@rails/ujs": "^6.0.0",
    "@rails/webpacker": "4.2.2",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "@types/node": "^13.11.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.1.2",
    "ts-loader": "^6.2.2",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest": "^25.3.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-resolve": "^25.3.0",
    "jest-watch-typeahead": "^0.5.0",
    "prettier": "^2.0.4",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": "13.x",
    "yarn": "1.22.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "plugins": [
      "react",
      "@typescript-eslint",
      "prettier"
    ],
    "env": {
      "browser": true,
      "jasmine": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "semi": false,
          "singleQuote": true,
          "jsxSingleQuote": true,
          "printWidth": 144
        }
      ],
      "@typescript-eslint/triple-slash-reference": [
        "error",
        {
          "path": "always",
          "types": "always",
          "lib": "always"
        }
      ],
      "@typescript-eslint/camelcase": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-console": "off",
      "react/prop-types": "off"
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}
